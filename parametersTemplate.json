{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "namespaceName": {
            "value": "$namespaceName"
        },
        "location": {
            "value": "$location"
        },
        "sku": {
            "value": "$sku"
        },
        "messagingUnits": {
            "value": "$messagingUnits"
        },
        "eventHubName": {
            "value": "$eventHubName"
        },
        "messageRetentionInDays": {
            "value": $messageRetentionInDays
        },
        "partitionCount": {
            "value": $partitionCount
        }
    }
}
